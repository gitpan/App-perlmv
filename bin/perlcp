#!/usr/bin/perl

use strict;
use warnings;
use FindBin '$Bin';
use lib "$Bin/../lib";
use App::perlmv;

our $VERSION = '0.44'; # VERSION

my $pmv = App::perlmv->new;
$pmv->{ 'mode' } = 'copy';
$pmv->run;

1;
# ABSTRACT: Copy files using Perl code
# PODNAME: perlcp


__END__
=pod

=head1 NAME

perlcp - Copy files using Perl code

=head1 VERSION

version 0.44

=head1 DESCRIPTION

See L<perlmv> for more documentation.

