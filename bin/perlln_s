#!perl

use strict;
use warnings;
use FindBin '$Bin';
use lib "$Bin/../lib";
use App::perlmv;

our $VERSION = '0.47'; # VERSION

my $pmv = App::perlmv->new;
$pmv->{ 'mode' } = 'symlink';
$pmv->run;

1;
# ABSTRACT: Symlink files using Perl code
# PODNAME: perlln_s

__END__

