#!perl

use strict;
use warnings;
use FindBin '$Bin';
use lib "$Bin/../lib";
use App::perlmv;

our $VERSION = '0.48'; # VERSION

my $pmv = App::perlmv->new;
$pmv->{ 'mode' } = 'rename';
$pmv->run;

1;
# ABSTRACT: Rename files using Perl code
# PODNAME: perlrename

__END__

=pod

=encoding UTF-8

=head1 NAME

