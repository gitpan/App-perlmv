#!/usr/bin/perl

use strict;
use warnings;
use FindBin '$Bin';
use lib "$Bin/../lib";
use App::perlmv;

our $VERSION = '0.45'; # VERSION

my $pmv = App::perlmv->new;
$pmv->{ 'mode' } = 'link';
$pmv->run;

1;
# ABSTRACT: Link files using Perl code
# PODNAME: perlln


__END__
=pod

=head1 NAME

perlln - Link files using Perl code

=head1 VERSION

version 0.45

=head1 DESCRIPTION

See L<perlmv> for more documentation.

